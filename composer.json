{
    "name": "visanduma/nova-image-tinymce",
    "description": "A Laravel Nova TinyMCE field with built in image gallery support",
    "homepage": "https://github.com/visanduma/nova-image-tinymce",
    "keywords": [
        "laravel",
        "nova",
        "tinymce",
        "editor",
        "wysiwyg",
        "image",
        "image-picker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LaHiRu",
            "email": "lahiru@visanduma.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Visanduma\\NovaImageTinymce\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Visanduma\\NovaImageTinymce\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
